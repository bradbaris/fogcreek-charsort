var chars = Array.from("abcdefghijklmnopqrstuvwxyz_");
var counter = [];

function letterCount() {
  for(var i = 0; i < chars.length; i++) {
    counter.push(
      { 
        letter: chars[i],
        count : string.split(chars[i]).length - 1
      }
    );
  }
}

function solveWord() {
  letterCount();
  counter.sort(function (a, b) { return b.count - a.count });
  counter = counter.slice(0, counter.indexOf(counter.find(function(item){ return item.letter === "_" }))).map(function(current) { return current.letter }).join("");
  console.log(counter);
  return counter;
}

solveWord();
